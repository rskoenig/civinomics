         ref.parentNode.insertBefore(js, ref);
       }(document));
    </script>    
     
  
   
   ${self.navbar()}   
     
    <div id="wrap">
        <div id="container">
            <div class="row"><!-- main content -->
                <div id="left_navigbar" class="span2">
                    ${self.display_navigbarfn()}
                </div>
                <div id="right_content" class="span9">
                    ${self.display_body_right_content()}
                </div>
            </div><!-- close row--> 
        </div><!--close container-->
    </div><!--close wrap-->
    

  ${self.footer()}
  
    <script src="/js/bootstrap/jquery.js"></script>
    <script src="/js/bootstrap/bootstrap-modal.js"></script> <!-- bootstrap modal scripts -->
    
    <script>
        function closeDialog () {
        $('#windowTitleDialog').modal('hide'); 
        };
        function okClicked () {
        closeDialog ();
        };
    </script>
    
    <!-- additional social media scripts -->
    <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");
    </script>

    <script type="text/javascript">
      (function() {
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = 'https://apis.google.com/js/plusone.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
      })();
    </script>

    <script>
        function closeDialog () {
            $('#windowTitleDialog').modal('hide'); 
            };
        function okClicked () {
            closeDialog ();
            };
