<%def name="display_navigbarfn()">
    <div class="span2">
      <div class="well sidebar-nav" style="padding-top:10px; padding-left:14px; padding-right:14px;">
        <ul class="nav nav-list" >
            % if display_navigbar == True:
                <li class="nav-header">Organization</li>
                <li 
                % if highlight_name == "about":
                    class ="active"
                %endif
                ><a href="/corp/about">About Us</a></li>
                <li><a href="http://www.civinomics.wordpress.com" target="_blank">Development Blog</a></li>
                <li 
                % if highlight_name == "careers":
                   class ="active"
                %endif
                ><a href="/corp/careers">Careers</a></li>
                <li 
                % if highlight_name == "team":
                   class ="active"
                %endif
                ><a href="/corp/team">Civinomics Team</a></li>
                <li 
                % if highlight_name == "terms":
                   class ="active"
                %endif
                ><a href="/corp/terms">Terms of Use</a></li>
                <li 
                % if highlight_name == "privacy":
                   class ="active"
                %endif
                ><a href="/corp/privacy">Privacy Policy</a></li>
                <li class="nav-header">Services</li>
                <li
                % if highlight_name == "outreach":
                    class ="active"
                %endif
                ><a href="/corp/outreach">Outreach</a></li>
                % endif
        </ul>
      </div><!--/.well -->
    </div><!--/span sidebar-->
</%def>

